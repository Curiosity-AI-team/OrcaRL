<launch>

    <!-- launch camera + yolo -->
    <include file="$(find depthai_examples)/launch/yolov4_publisher.launch">
        <arg name="camera_model" value="OAK-D-LITE" />
    </include>

    <!-- launch imu -->
    <arg name="calib_file" default="/home/jetson/catkin_ws/src/OrcaRL/sensors/mpu9250_ros/config/calib.json" doc="Path to the calibration file"/>
    <node name="mpu_node" pkg="mpu9250_ros" type="mpu_node.py" output="screen"  args="$(arg calib_file)">
    </node>

    <!-- <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch"/>

    <node name="tf2_web_republisher" pkg="tf2_web_republisher" type="tf2_web_republisher" output="screen"/> -->

</launch>